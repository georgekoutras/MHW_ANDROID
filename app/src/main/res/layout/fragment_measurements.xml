                    android:layout_weight="0.2"
                    android:layout_gravity="center_vertical|center_horizontal"
                    >

                    <ImageView
                        android:id="@+id/img_glucometer"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/icon_glucometer"
