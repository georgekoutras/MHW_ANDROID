                <LinearLayout
                    android:id="@+id/stress_body_data"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.7"
                    android:visibility="gone"
                    android:orientation="vertical"
                    android:layout_gravity="center_vertical">

                    <LinearLayout
                        android:id="@+id/stress_list_headings"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/stress_list_heading1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:text="@string/stress_list_heading"
                            android:textSize="14sp" />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/stress_list_data"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:weightSum="1"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/stress_list_data1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="14sp"
                            />

                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:id="@+id/stress_more"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="0.1"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    android:tint="@color/basic_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center_vertical"/>

                <ImageView
                    android:id="@+id/stress_activate"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="0.1"
                    android:src="@drawable/ic_baseline_assignment_24"
                    android:tint="@color/basic_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center_vertical"
                    android:visibility="gone"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/cough_measurement"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/stress_measurement"
                android:background="@drawable/border_shape"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="horizontal"
                android:weightSum="1"
                >

                <RelativeLayout
                    android:id="@+id/cough_icon_set"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2"
                    android:layout_gravity="clip_horizontal|center_vertical">

                    <ImageView
                        android:id="@+id/img_cough"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:src="@drawable/icon_cough"
                        android:layout_centerHorizontal="true"/>
                    <TextView
                        android:id="@+id/title_cough"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/img_cough"
                        android:text="@string/cough"
                        android:textColor="@color/black"
                        android:layout_centerHorizontal="true"/>

                </RelativeLayout>

                <TextView
                    android:id="@+id/cough_body"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.7"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:layout_gravity="center_vertical"/>

                <ImageView
                    android:id="@+id/cough_more"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="0.1"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    android:tint="@color/basic_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center_vertical"/>

                <ImageView
                    android:id="@+id/cough_activate"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="0.1"
                    android:src="@drawable/ic_baseline_assignment_24"
                    android:tint="@color/basic_color"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_gravity="center_vertical"
                    android:visibility="gone"/>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>