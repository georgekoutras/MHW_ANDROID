<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentMeasurements"
    android:layout_marginTop="10dp">

    <TextView
        android:id="@+id/measurements_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/measurements_title"
        android:textStyle="bold"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_marginLeft="10dp"
        />
    <RelativeLayout
        android:id="@+id/measurements_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="15dp"
        android:layout_below="@id/measurements_header">
        <TextView
            android:id="@+id/measurements_date_field"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:clickable="true"
            android:focusable="true"
            android:drawableTint="@color/basic_color"
            />
        <ImageView
            android:id="@+id/prev_date"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/ic_baseline_arrow_left_24"
            android:tint="@color/basic_color"
            android:clickable="true"
            android:focusable="true"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/measurements_date_field"
            />

        <ImageView
            android:id="@+id/next_date"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/ic_baseline_arrow_right_24"
            android:tint="@color/basic_color"
            android:clickable="true"
            android:focusable="true"
            android:layout_toRightOf="@id/measurements_date_field"
            android:layout_centerVertical="true"
            android:visibility="gone"
            />
    </RelativeLayout>
    <ScrollView
        android:id="@+id/edit_account_scrollview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/measurements_date">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
